<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
        xmlns:xs="http://www.w3.org/2001/XMLSchema" 
        xmlns:tns="http://www.example.org/CTE" 
        targetNamespace="http://www.example.org/CTE" 
        elementFormDefault="qualified">

	<xs:redefine schemaLocation="ComplexType_Extension_Base.xsd">
		<xs:complexType name="pname">
	    	<xs:complexContent>
	      		<xs:extension base="tns:pname">
	        		<xs:sequence>
	         			 <xs:element name="country" type="xs:string"/>
	        		</xs:sequence>
	      		</xs:extension>
	    	</xs:complexContent>
	  	</xs:complexType>	
	</xs:redefine>
</schema>